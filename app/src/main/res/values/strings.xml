    <string name="text_covid_high4">Call - 1075</string>

    <string name="text_covid_mod1">Medium Risk</string>
    <string name="text_covid_mod2">Based on the responses received, your chances of being infected with corona virus is <b>Moderate</b>. Keep a close watch on your symptoms and quarantine yourself. Follow the recommendations to protect yourself and others. Keep Calm, Stay Healthy!</string>
    <string name="text_covid_mod3">Isolate Yourself Just in Case</string>
    <string name="text_covid_mod4">Help Call - 1075</string>

    <string name="text_covid_low1">Low Risk</string>
    <string name="text_covid_low2">Based on the responses recorded, your chances of being infected with Corona Virus is <b>LOW</b>. To remain safe and protected, follow the preventive recommendations. Keep Calm, Stay Healthy!</string>
    <string name="text_covid_low3"><b>Stay Quarantined</b> in your home.</string>
    <string name="text_covid_low4">In case of Help Call - 1075</string>

